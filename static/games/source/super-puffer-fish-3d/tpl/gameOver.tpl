<section id="gameOver" class="screen">
	<div class="mute scale scale-top-left">
		<button></button>
	</div>

	<div class="wrapper scale scale-top-center">
		<div class="frame">
			<h1 class="score">Your Score</h1>
			<p class="score">0</p>
			<input class="name" type="text" placeholder="Your name" maxlength="15"/>
		</div>

		<div class="goodJobFrame">
			<h1 class="goodJobHeader">Tolle Leistung!</h1>
			<p class="goodJobText">
				<span class="notSubmitted">Trage deine Punkte <br/> in die Bestenliste ein!</span>
				<span class="submitted">Gleich noch mal!</span>
			</p>
		</div>

		<div class="character">	</div>

		<div class="buttons">
			<button class="instructions"></button>
			<button class="play"></button>
			<button class="upload"></button>
			<button class="highscore"></button>
		</div>

	</div>
</section>
