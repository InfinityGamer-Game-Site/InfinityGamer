<section id="inbetween" class="screen">
	<div class="mute scale scale-top-left">
		<button></button>
	</div>

	<div class="wrapper-main scale scale-top-center">
		<div class="frame">
			<h1 class="score">Your Score</h1>
			<p class="score">0</p>

			<div class="pickups">10</div>
			<div class="time">0:00</div>
		</div>

		<div class="stars">
			<div class="star1"></div>
			<div class="star2"></div>
			<div class="star3"></div>
		</div>


		<div class="character">	</div>

		<div class="buttons">
			<button class="map"></button>
			<button class="next"></button>
			<button class="again"></button>
		</div>
	</div>

	<div class="wrapper-popup scale scale-center">
		<div class="frame">
		    <div class="text">Congratulation, you played all the levels! More will follow soon...</div>
			<button class="okay"></button>
		</div>
	</div>



</section>
